PREFIX snvoc: <http://www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/>

SELECT *
WHERE {
    ?p1 a snvoc:Person .
    ?p2 a snvoc:Person .
    ?p1 snvoc:workAt ?w1 .
    ?p2 snvoc:workAt ?w2 .
    ?w1 snvoc:hasOrganisation ?c .
    ?w2 snvoc:hasOrganisation ?c .
    ?p1 snvoc:studyAt ?s1 .
    ?p2 snvoc:studyAt ?s2 .
    ?s1 snvoc:hasOrganisation ?u .
    ?s2 snvoc:hasOrganisation ?u .
}