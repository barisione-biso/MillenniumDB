PREFIX snvoc: <http://www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/>

SELECT
    ?p1Id
    ?p2Id
    ?uId
WHERE {
    ?p1 a snvoc:Person .
    ?p2 a snvoc:Person .
  FILTER(?p1 != ?p2) .
    ?p1 snvoc:studyAt ?s1 .
    ?p2 snvoc:studyAt ?s2 .
    ?s1 snvoc:hasOrganisation ?u .
    ?s2 snvoc:hasOrganisation ?u .
    ?p1 snvoc:id ?p1Id .
    ?p2 snvoc:id ?p2Id .
    ?u snvoc:id ?uId .
}