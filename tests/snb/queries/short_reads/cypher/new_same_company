MATCH (p1:Person)-[:WORK_AT]->(c:Organisation {type:"company"})<-[:WORK_AT]-(p2:Person)
RETURN
  p1.id,
  p2.id,
  c.id;


PROFILE MATCH (p1:Person)-[:WORK_AT]->(c:Organisation {type:"company"})<-[:WORK_AT]-(p2:Person)
WHERE p1.id <> p2.id
RETURN count(*) as count;

MATCH (p1:Person)-[:WORK_AT]->(c:Organisation {type:"company"})<-[:WORK_AT]-(p2:Person)
RETURN count(*) as count;