// Original: People born before 1880 with no death date
// tiene un FILTER NOT EXISTS que no soportamos
// Modificada: Personas nacidas antes de 1880 y su fecha de muerte
SELECT ?h, ?dateB.time, ?dateD.time
MATCH (?h)-[:P31]->(Q5),
      (?h)-[:P569]->(?dateB)
OPTIONAL{
      (?h)-[:P570]->(?dateD)
}
WHERE ?dateB.time < "+1880-01-01T00:00:00Z"
LIMIT 100