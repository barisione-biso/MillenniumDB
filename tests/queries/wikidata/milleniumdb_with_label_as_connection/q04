// Original: Cities with female mayors ordered by population
// Modified, missing property path, FILTER NOT EXISTS and GROUP BY
SELECT DISTINCT ?city, ?citylabel.string_value, ?mayorlabel.string_value//, ?pop.amount
MATCH (?city)-[:P31]->(Q515),
      (?city)-[:P6]->(?mayor),
      (?mayor)-[:P21]->(Q6581072),
      (?city)-[:P1082]->(?pop)
OPTIONAL {
      (?city)-[:Label {language:"en"}]->(?citylabel),
      (?mayor)-[:Label {language:"en"}]->(?mayorlabel)
}
LIMIT 100