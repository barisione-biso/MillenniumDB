// SELECT ?state ?label ?borders
// WHERE {
//     {
//         SELECT ?state (COUNT(?neigh) as ?borders)
//         WHERE {
//             <?state :P31 :Q35657 ?i1> .
//             <?neigh :P47 ?state _:i2> .
//             <?neigh :P31 :Q35657 ?i3 > .
//         }
//         GROUP BY ?state
//     }
//     OPTIONAL { ?state rs:label ?label . FILTER(lang(?label) = "en") }
// }
// ORDER BY DESC(?borders)
SELECT ?state, ?label.string_value
MATCH (?state)-[:P31]->(Q35657),
      (?neigh)-[:P47]->(?state),
      (?neigh)-[:P31]->(?Q35657),
      (?state)-[:Label]->(?label)
LIMIT 100