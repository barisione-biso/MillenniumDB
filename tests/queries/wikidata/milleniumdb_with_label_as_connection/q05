SELECT ?country, ?countrylabel.string_value
MATCH (?city)-[:P31]->(Q515),
      (?city)-[?e1 :P6]->(?mayor),
      (?e1)-[:P582]->(?x),
      (?mayor)-[:P21]->(?Q6581072),
      (?city)-[:P17]->(?country),
      (?country)-[:Label]->(?countrylabel)
LIMIT 100