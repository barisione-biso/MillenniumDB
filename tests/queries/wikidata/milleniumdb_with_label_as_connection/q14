// SELECT ?person ?name ?uname
// WHERE {
//     <?person :P553 wd:Q52 ?i > .
//     ?i :P554q ?uname .
//     ?person rs:label ?name . FILTER(LANG(?name) = "en") .
// } LIMIT 100
SELECT ?person, ?name.string_value, ?uname.string_value
MATCH (?person)-[?e :P553]->(Q52),
      (?e)-[:P554]->(?uname),
      (?person)-[:Label]->(?name)
LIMIT 100