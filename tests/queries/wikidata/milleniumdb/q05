// Original: Countries ordered by number of female city mayors
// Modified, missing property path, FILTER NOT EXISTS and GROUP BY
SELECT ?country, ?countrylabel
MATCH (?city)-[P31]->(Q515),
      (?city)-[P6]->(?mayor),
      (?mayor)-[P21]->(?Q6581072),
      (?city)-[P17]->(?country)
OPTIONAL {
      (?country)-[Label {language:"en"}]->(?countrylabel)
}
LIMIT 100
