MATCH (?x) -[?e :PERSON]->(Q123)
WHERE ?x.name == "John"


SELECT *
MATCH (Q123) -[?e :PERSON]->(?y)
WHERE ?y.name == "John"


SELECT *
MATCH (Q123) -[?e :PERSON]-> (Q122)
WHERE ?e.name == "bla"



